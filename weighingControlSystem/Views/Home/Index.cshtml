<script src="~/Scripts/jquery-1.12.3.min.js"></script>
<script src="~/Content/Theams/plugins/jquery-datatable/jquery.dataTables.js"></script>
<link href="~/Content/Theams/plugins/jquery-datatable/skin/bootstrap/css/dataTables.bootstrap.css" rel="stylesheet" />
<link href="~/Content/jquery.dataTables.css" rel="stylesheet" />
<script src="~/Content/Theams/js/pages/forms/basic-form-elements.js"></script>
<script type="text/javascript">

    var dataTableLastpageObject = $('.js-basic-example').DataTable({
        "processing": true, // for show progress bar
        "serverSide": true, // for process server side
        "filter": false, // this is for disable filter (search box)
        "orderMulti": false, // for disable multiple column at once
        "bLengthChange": false,
        "bFilter": true,
        "bInfo": false,
        "ajax": {
            "url": "/Home/getTruckWorkingList",
            "type": "POST",
            "datatype": "json"
        },
        "columns": [
                { "data": "TruckNo", "name": "TruckNo", "orderable": false, "autoWidth": true },
                { "data": "noTripShifA", "name": "noTripShifA", "autoWidth": true },
                { "data": "TotalTonShiftA", "name": "TotalTonShiftB", "autoWidth": true },
                { "data": "ToltalFillFactorA", "name": "ToltalFillFactorA", "autoWidth": true },
                { "data": "noTripShifB", "name": "noTripShifB", "autoWidth": true },
                { "data": "TotalTonShiftB", "name": "TotalTonShiftB", "autoWidth": true },
                { "data": "ToltalFillFactorB", "name": "ToltalFillFactorB", "autoWidth": true },
                { "data": "noTripShifC", "name": "noTripShifC", "autoWidth": true },
                { "data": "TotalTonShiftC", "name": "TotalTonShiftC", "autoWidth": true },
                { "data": "ToltalFillFactorC", "name": "ToltalFillFactorC", "autoWidth": true },
                { "data": "noTripShifFD", "name": "noTripShifFD", "autoWidth": true },
                { "data": "TotalTonShiftFD", "name": "TotalTonShiftFD", "autoWidth": true },
                {
                    "data": "ToltalFillFactorFD", "name": "ToltalFillFactorFD", "autoWidth": true, "render": function (data, type, full, meta) {
                        var mg = '@ViewBag.Msg';
                        if (mg != '') {
                            alert(mg);
                        }
                        return data;
                    }
                }
        ]       
    })
    $(document).ready(function () {
        debugger;
        var mg = '@ViewBag.Msg';
        if (mg != '')
        {
            alert(mg);
        }
    })
</script>
